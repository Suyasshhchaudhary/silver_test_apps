<div class="page-header">
  <%= link_to posts_path, class: 'btn btn-default' do %>
    All Posts
  <% end %>
  <%= link_to edit_post_path(@post), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show post</h1>
</div>

<dl class="dl-horizontal">
  
  <dt>title:</dt>
  <dd><%= @post.title %></dd>
  
  <dt>age:</dt>
  <dd><%= @post.age %></dd>
  
  <dt>desc:</dt>
  <dd><%= @post.desc %></dd>
  
  <dt>sex:</dt>
  <dd><%= @post.sex %></dd>
  
</dl>
